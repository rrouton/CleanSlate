<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/logged_out_nav_graph" app:startDestination="@id/mainFragment">

    <fragment android:id="@+id/mainFragment" android:name="com.rrouton.android.cleanslate.ui.main.MainFragment"
              android:label="MainFragment">
        <action android:id="@+id/action_mainFragment_to_countdownFragment" app:destination="@id/countdownFragment"
                app:enterAnim="@anim/nav_default_enter_anim" app:exitAnim="@anim/nav_default_exit_anim"
                app:popEnterAnim="@anim/nav_default_pop_enter_anim" app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment android:id="@+id/countdownFragment"
              android:name="com.rrouton.android.cleanslate.ui.countdown.CountdownFragment"
              android:label="CountdownFragment"/>
</navigation>